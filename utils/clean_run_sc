rm onetep_dmft_part* > /dev/null 2>&1
rm onetep_dimer_splitting* > /dev/null 2>&1
rm onetep_projecting_back*  > /dev/null 2>&1
rm -r sc_dmft_full_iter* > /dev/null 2>&1
