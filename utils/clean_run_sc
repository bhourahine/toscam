rm updating_variables > /dev/null 2>&1
rm collect_rank_files   > /dev/null 2>&1
rm summing_dmft_energy* > /dev/null 2>&1
rm run.dkn_onetep > /dev/null 2>&1
rm run.dkn_dmft > /dev/null 2>&1
rm summing_up_contribN* > /dev/null 2>&1  
rm sum_matrixDensKernelc* > /dev/null 2>&1
rm invert_matc* > /dev/null 2>&1

